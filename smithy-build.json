{
    "version": "1.0",
    "sources": ["model"],
    "maven": {
        "dependencies": [
            "software.amazon.smithy:smithy-openapi:1.61.0",
            "software.amazon.smithy.typescript:smithy-typescript-codegen:0.34.1"
        ]
    },
    "plugins": {
        "openapi": {
            "service": "ryan.vehicle#Vehicle",
            "synthesizeEnumNames": true,
            "ignoreUnsupportedTraits": true
        },
        "typescript-client-codegen": {
            "package": "@smithy/typescript-example-client",
            "packageVersion": "0.0.1"
        }
    }
}